{
  "include": ["src/"],
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES6", "ES2019.Array"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "skipLibCheck": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  }
}
